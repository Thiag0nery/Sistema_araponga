{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "handle": "filesystem",
        "src": "/css/(.*)",
        "dest": "/public/css/"
      },
      {
        "handle": "filesystem",
        "src": "/img/(.*)",
        "dest": "/public/img/"
      },
      {
        "handle": "filesystem",
        "src": "/js/(.*)",
        "dest": "/public/js/"
      },
      {
        "src": "/favicon.ico",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/manifest.json",
        "dest": "/public/manifest.json"
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  }