{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/src/public/(css|js|img)/(.*)",
      "dest": "src/public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
