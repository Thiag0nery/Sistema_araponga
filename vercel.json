{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    
    ],
    "routes": [
      { "handle": "@vercel/node" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }