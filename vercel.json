{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/css/**", 
        "use": "@vercel/static"
      },
      {
        "src": "public/img/**", 
        "use": "@vercel/static" 
      },
      {
        "src": "public/js/**", 
        "use": "@vercel/static" 
      }
    
    ],
    "routes": [
      {
     
        "src": "/css/(.*)",
        "dest": "/public/css/"
      },
      {
       
        "src": "/img/(.*)",
        "dest": "/public/img/"
      },
      {
       
        "src": "/js/(.*)",
        "dest": "/public/js/"
      },
      {
        "src": "/favicon.ico",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/manifest.json",
        "dest": "/public/manifest.json"
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  }