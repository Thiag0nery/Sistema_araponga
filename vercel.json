{
  "version": 2,
  
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
   
  
  ],
  
  
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/public/(css|js|img)/(.*)",
        "dest": "public/$1/$2"
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  
  
  
}