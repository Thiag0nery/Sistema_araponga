{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/public/img/(.*)",
      "dest": "public/img/$1"
    }
  ]
}
